[gui]
  tabsize = 2
[alias]
  i = init
  conf = config
  s = status
  b = branch
  ba = branch -a
  f = fetch
  df = diff
  dfc = diff --cached
  g = grep
  r = remote
  rpo = remote prune origin
  rv = remote -v
  sh = show
  st = stash
  stl = stash list
  ad = add
  ci = commit
  co = checkout
  sw = switch
  l = log --graph
  lo = log origin
  la = log --graph --all --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(bold white)â€• %an%C(reset)%C(bold yellow)%d%C(reset)' --abbrev-commit --date=relative
  lp = log --graph -p
[color]
  status = auto
  branch = auto
  diff = auto
  grep = auto
[pager]
  log = diff-highlight | less
  show = diff-highlight | less
  diff = diff-highlight | less
[core]
  editor = vim
  quotepath = false
  pager = less
[merge]
  tool = vimdiff
  squash = true
[fetch]
  prune = true
[pull]
  rebase = true
[init]
  defaultBranch = main
[credential]
  helper = osxkeychain
[rebase]
  autosquash = true
